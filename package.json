{
	"name": "heart-api",
	"version": "1.3.7",
	"description": "API that can be used to retrieve various data that is found in a variety of places. An ideal use case is to retrieve social media information for specific services.",
	"main": "./src/app.ts",
	"type": "module",
	"scripts": {
		"fresh-install": "rm -rf node_modules; npm install --no-optional",
		"run-mocha-tests": "c8 -c './config/.nycrc.json' mocha --config './config/mocharc.json' 'unit-test/**/*.ts'",
		"test": "env-cmd -e test npm run run-mocha-tests",
		"mutation": "NODE_OPTIONS='--loader ts-node/esm' env-cmd -e test stryker run config/stryker.conf.json",
		"integration-test": "source integration-test/jwt.sh; NODE_OPTIONS='--loader ts-node/esm' env-cmd -e test cucumber-js --config config/cucumber.mjs",
		"start": "env-cmd -e dev nodemon --ext ts --exec ts-node ./src/app.ts",
		"start-prod": "env-cmd -e prod nodemon --ext ts --exec ts-node ./src/app.ts",
		"build": "rm -rf dist && tsc && cp ./package.json ./dist && cp ./.env-cmdrc.json ./dist && cp -r ./certs ./dist && cp ./tsconfig.json ./dist",
		"upload-production": "npm run build && bash artifact-transfer.sh 'ec2-3-20-117-123.us-east-2.compute.amazonaws.com'",
		"restage-production": "bash docker-restage.sh 'ec2-3-20-117-123.us-east-2.compute.amazonaws.com'",
		"deploy-production": "npm run upload-production && npm run restage-production",
		"start-built-api": "env-cmd -e prod node -r ts-node/register ./src/app.js",
		"local-start": "npm run build && cd ./dist && npm run start-built-api",
		"docker-start": "npm run start-built-api",
		"doppler:setup-env": "bash doppler-secrets-local-setup.sh prod",
		"doppler:update-env": "bash doppler-update-secrets.sh prod"
	},
	"keywords": [
		"heart",
		"api"
	],
	"author": "Javi Gomez",
	"license": "ISC",
	"dependencies": {
		"axios": "1.3.4",
		"cors": "2.8.5",
		"express": "4.18.2",
		"fs": "0.0.2",
		"http": "0.0.1-security",
		"https": "1.0.0",
		"jsonwebtoken": "9.0.0",
		"lodash.sample": "4.2.1",
		"lodash.uniq": "^4.2.0",
		"logger": "0.0.1",
		"mongoose": "7.0.0",
		"morgan": "1.10.0",
		"nano-memoize": "3.0.8"
	},
	"devDependencies": {
		"@cucumber/cucumber": "9.0.0",
		"@stryker-mutator/core": "6.4.1",
		"@stryker-mutator/mocha-runner": "6.4.1",
		"@stryker-mutator/typescript-checker": "6.4.1",
		"@types/chai": "4.3.4",
		"@types/chai-http": "4.2.0",
		"@types/cors": "2.8.13",
		"@types/cucumber": "7.0.0",
		"@types/express": "4.17.17",
		"@types/jsonwebtoken": "9.0.1",
		"@types/lodash.sample": "4.2.7",
		"@types/lodash.uniq": "4.5.7",
		"@types/mocha": "10.0.1",
		"@types/morgan": "1.9.4",
		"@types/node": "18.14.6",
		"@types/sinon": "10.0.13",
		"@types/supertest": "2.0.12",
		"c8": "7.13.0",
		"chai": "4.3.7",
		"chai-http": "4.3.0",
		"mocha": "10.2.0",
		"mocha-suppress-logs": "0.3.1",
		"nodemon": "2.0.21",
		"pactum": "3.3.2",
		"sinon": "15.0.1",
		"supertest": "6.3.3",
		"ts-node": "10.9.1",
		"tslint": "6.1.3",
		"typescript": "4.9.5"
	}
}
